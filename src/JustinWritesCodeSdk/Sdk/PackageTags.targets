<Project>
  <Target Name="SetPackageTags" BeforeTargets="GetPackageMetadata;GenerateMSBuildEditorConfigFileShouldRun">
    <PropertyGroup>
      <PackageTags>@(PackageTag->Distinct(), ' ')</PackageTags>
    </PropertyGroup>
  </Target>
</Project>
